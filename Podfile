# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'LiveChatMap' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LiveMap
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'SwiftyJSON'
  pod 'Kingfisher'
  pod 'Socket.IO-Client-Swift'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'SVProgressHUD'
  pod 'SDCAlertView'
end
